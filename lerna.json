{
  "command": {
    "bootstrap": {
      "loglevel": "silent"
    },
    "run": {
      "loglevel": "silent",
      "stream": false
    },
    "publish": {
      "allowBranch": [
        "develop",
        "master"
      ],
      "conventionalCommits": true,
      "ignoreChanges": [
        "*.md",
        "LICENSE"
      ],
      "loglevel": "verbose",
      "message": "chore(release): publish",
      "yes": true
    },
    "version": {
      "allowBranch": [
        "develop",
        "master"
      ],
      "conventionalCommits": true,
      "ignoreChanges": [
        "*.md",
        "LICENSE"
      ],
      "loglevel": "verbose",
      "message": "chore(release): publish",
      "yes": true
    }
  },
  "packages": [
    "binaries/*"
  ],
  "version": "independent"
}