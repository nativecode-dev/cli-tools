{
  "command": {
    "run": {
      "stream": true
    },
    "publish": {
      "allowBranch": [
        "develop",
        "master",
        "feature/"
      ],
      "conventionalCommits": true,
      "ignoreChanges": [
        "*.md",
        "CHANGELOG.md",
        "LICENSE",
        "README.md",
        ".npmignore",
        "package-lock.json",
        "tasks.json",
        "tsconfig.json"
      ],
      "loglevel": "verbose",
      "message": "[skip ci] chore(release): publish",
      "yes": true
    },
    "version": {
      "allowBranch": [
        "develop",
        "master",
        "feature/"
      ],
      "conventionalCommits": true,
      "ignoreChanges": [
        "*.md",
        "CHANGELOG.md",
        "LICENSE",
        "README.md",
        ".npmignore",
        "package-lock.json",
        "tasks.json",
        "tsconfig.json"
      ],
      "loglevel": "verbose",
      "message": "[skip ci] chore(release): publish",
      "yes": true
    }
  },
  "loglevel": "error",
  "npmClient": "npm",
  "npmClientArgs": [
    "--no-progress"
  ],
  "packages": [
    "binaries/*"
  ],
  "version": "independent"
}
