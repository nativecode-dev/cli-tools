{
  "command": {
    "publish": {
      "allowBranch": [
        "develop",
        "master",
        "feature/"
      ],
      "conventionalCommits": true,
      "ignoreChanges": [
        "*.md",
        ".ci-env.sh",
        ".editorconfig",
        ".gitattributes",
        ".gitignore",
        ".gitlabg-ci-yml",
        ".gitmodules",
        ".mocharc.json",
        ".npmignore",
        ".prettierrc",
        "lerna.json",
        "LICENSE",
        "package-lock.json",
        "tasks.json",
        "tsconfig.json",
        "tsconfig.settings.json",
        "tslint.json"
      ],
      "loglevel": "verbose",
      "message": "[skip ci] chore(release): publish",
      "yes": true
    },
    "run": {
      "stream": true
    }
  },
  "loglevel": "error",
  "npmClient": "npm",
  "npmClientArgs": [
    "--no-progress",
    "--silent"
  ],
  "packages": [
    "binaries/*"
  ],
  "version": "independent"
}
