{
  "tasks": {
    "build": [
      "[build:ts]",
      "[shebang]"
    ],
    "build:ts": [
      "tsc --project tsconfig.json"
    ],
    "clean": [
      "rimraf bin",
      "rimraf *.tsbuildinfo"
    ],
    "exec": [
      "ts-node src/cli-env"
    ],
    "shebang": [
      "ts-node ../tasks/src/cli-shebang"
    ]
  }
}
